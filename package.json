{
  "name": "api-doc-agent",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "api-doc-agent": "./dist/cli.js"
  },
  "scripts": {
    "validate": "ts-node ./scripts/validate-codebase.ts",
    "build": "tsc -p tsconfig.json",
    "extract": "ts-node ./scripts/extract-metadata.ts",
    "normalize": "ts-node ./scripts/normalize-metadata.ts",
    "enrich": "echo '⚠️  DEPRECATED: Use enrich-agentic for better AI-generated documentation' && ts-node ./scripts/enrich-metadata.ts",
    "enrich-agentic": "ts-node ./scripts/enrich-metadata-agentic.ts",
    "enrich-simple": "ts-node ./scripts/legacy/enrich-metadata-simple.ts",
    "generate": "ts-node ./scripts/generate-docs.ts",
    "test": "jest",
    "cli": "ts-node ./src/cli.ts",
    "agents:status": "ts-node -e \"import('./src/agent-config').then(({loadAgentConfig}) => { const cfg = loadAgentConfig(); console.log('Available providers:', Object.keys(cfg.providers)); })\"",
    "agents:test": "ts-node ./scripts/test-agents.ts",
    "agents:test-individual": "ts-node ./scripts/test-individual-agents.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.73.0",
    "@google/genai": "^1.40.0",
    "dotenv": "^16.4.5",
    "openai": "^4.28.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.5.1",
    "commander": "^11.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "ts-morph": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
